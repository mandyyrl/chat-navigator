{
  "manifest_version": 3,
  "name": "Squirrel Jump",
  "version": "1.0.0",
  "description": "A chrome extension that enhances AI chatbot (ChatGPT, Gemini, etc.) interfaces with a smart scrollbar and timeline navigation. Easily jump to any message, leveraging built-in summarizer APIs for efficient content preview and enhanced browsing within a single chat",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Squirrel Jump"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*",
        "https://chat.openai.com/*"
      ],
      "js": ["prompt-utils.js", "content-chatgpt.js"],
      "css": ["styles-chatgpt.css"]
    },
    {
      "matches": [
        "https://chat.deepseek.com/*"
      ],
      "js": ["content-deepseek.js"],
      "css": ["styles-deepseek.css"]
    },
    {
      "matches": [
        "https://gemini.google.com/*"
      ],
      "js": ["prompt-utils.js", "content-gemini.js"],
      "css": ["styles-gemini.css"]
    }
  ]
}
