{
  "manifest_version": 3,
  "name": "ChatGPT Conversation Timeline",
  "version": "1.0.0",
  "description": "Adds an navigation timeline to ChatGPT conversations for easy navigation.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "会话时间轴"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*",
        "https://chat.openai.com/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
